{
    "name": "dept/contao-powertools-url-tracking-params-bundle",
    "description": "Dept Powertools Url Tracking Params Bundle",
    "type": "contao-bundle",
    "authors": [{
        "name": "Jannis Ritterspach",
        "email": "jannis.ritterspach@deptagency.com",
        "homepage": "https://www.deptagency.com"
    }],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "^4.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Dept\\PowertoolsUrlTrackingParamsBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [ 
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Dept\\PowertoolsUrlTrackingParamsBundle\\ContaoManagerPlugin"
    },
    "replace": {
        "contao-legacy/powertools-url-tracking-params-bundle": "self.version"
    }
}